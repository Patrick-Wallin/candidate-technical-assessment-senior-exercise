{
  "name": "people-movies-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start-ts": "npx nodemon src/index.ts",
    "start-js": "node dist/src/index.js",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "migrate-up-prod": "db-migrate --env prod up",
    "migrate-down-prod": "db-migrate --env prod down",
    "migrate-up-dev": "db-migrate --env dev up",
    "migrate-down-dev": "db-migrate --env dev down",
    "jasmine-js": "jasmine dist/spec/**/*[sS]pec.js",
    "jasmine-ts": "jasmine-ts spec/**/*[sS]pec.ts",
    "test-dev-ts": "cross-env NODE_ENV=dev && npm run migrate-up-dev && npm run jasmine-ts && npm run migrate-down-dev",
    "jasmine-ts-people-service": "jasmine-ts spec/tests/services/peopleSpec.ts",
    "jasmine-ts-movies-service": "jasmine-ts spec/tests/services/moviesSpec.ts",
    "jasmine-ts-people-movies-service": "jasmine-ts spec/tests/services/peopleMoviesSpec.ts",
    "jasmine-ts-people-handler": "jasmine-ts spec/tests/handlers/peopleSpec.ts",
    "jasmine-ts-movies-handler": "jasmine-ts spec/tests/handlers/moviesSpec.ts",
    "jasmine-ts-people-movies-handler": "jasmine-ts spec/tests/handlers/peopleMoviesSpec.ts",
    "jasmine-ts-search-handler": "jasmine-ts spec/tests/handlers/searchSpec.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "@types/node": "^16.11.11",
    "@types/pg": "^8.6.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "pg": "^8.7.1",
    "typescript": "^4.5.2"
  }
}
